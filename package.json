{
  "name": "remote-dev",
  "version": "0.2.0",
  "description": "Web-based terminal interface with persistent tmux sessions and GitHub integration",
  "author": "Remote Dev Team",
  "private": true,
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "concurrently --kill-others --kill-signal SIGTERM -n next,term -c blue,green \"bun run dev:next\" \"bun run dev:terminal\"",
    "dev:next": "next dev --turbopack -p 6001",
    "dev:terminal": "TERMINAL_PORT=6002 tsx src/server/index.ts",
    "build": "next build",
    "start": "next start -p 6001",
    "start:terminal": "TERMINAL_PORT=6002 tsx src/server/index.ts",
    "rdv": "bun run scripts/rdv.ts",
    "rdv:dev": "bun run scripts/rdv.ts start dev",
    "rdv:prod": "bun run scripts/rdv.ts start prod",
    "rdv:stop": "bun run scripts/rdv.ts stop",
    "rdv:restart": "bun run scripts/rdv.ts restart",
    "rdv:status": "bun run scripts/rdv.ts status",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run src/db/seed.ts",
    "terminal:build": "bun build src/server/index.ts --outdir dist-terminal --target node --external node-pty --external better-sqlite3 --sourcemap",
    "electron:build": "tsc -p electron/tsconfig.json",
    "electron:dev": "bun run electron:build && concurrently --kill-others -n next,term,electron -c blue,green,yellow \"bun run dev:next\" \"bun run dev:terminal\" \"electron .\"",
    "electron:start": "bun run electron:build && electron .",
    "electron:rebuild": "electron-rebuild -f -w node-pty,better-sqlite3",
    "electron:pack": "bun run build && bun run terminal:build && bun run electron:build && electron-builder --dir",
    "electron:dist": "bun run build && bun run terminal:build && bun run electron:build && electron-builder",
    "electron:dist:mac": "bun run build && bun run terminal:build && bun run electron:build && electron-builder --mac",
    "electron:dist:linux": "bun run build && bun run terminal:build && bun run electron:build && electron-builder --linux",
    "electron:dist:win": "bun run build && bun run terminal:build && bun run electron:build && electron-builder --win"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.11.1",
    "@libsql/client": "^0.15.15",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@octokit/graphql": "^9.0.3",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/better-sqlite3": "^7.6.13",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-image": "^0.9.0",
    "@xterm/addon-search": "^0.16.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "croner": "^9.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "next-auth": "^5.0.0-beta.30",
    "node-pty": "^1.0.0",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.18.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "happy-dom": "^20.0.11",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "better-sqlite3",
    "node-pty",
    "sharp",
    "unrs-resolver"
  ]
}
