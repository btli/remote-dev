{
  "expo": {
    "name": "Remote Dev",
    "slug": "remote-dev",
    "version": "0.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "scheme": "remotedev",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1b26"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.remotedev.app",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Remote Dev uses Face ID to securely access your API keys"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1b26"
      },
      "package": "com.remotedev.app",
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-local-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#7aa2f7"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
